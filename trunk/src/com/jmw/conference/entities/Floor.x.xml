<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.jmw.conference.entities.Floor" table="FLOORS" lazy="false">
  <id column="FLOOR_ID" name="id">
   <generator class="native"/>
  </id>
  <property column="BUILDING_ID" generated="never" lazy="false" name="buildingId"/>
  <property generated="never" lazy="false" name="name"/>
  <set name="rooms" sort="unsorted" table="ROOMS" lazy="false">
   <key column="FLOOR_ID"/>
   <element column="ROOM_ID" type="integer"/>
  </set>
 </class>
</hibernate-mapping>
