<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.jmw.conference.entities.User" table="USERS" lazy="false">
  <id column="USER_ID" name="id">
   <generator class="native"/>
  </id>
  <property column="FIRST_NAME" name="firstName"/>
  <property column="LAST_NAME" name="lastName"/>
  <property column="PASSWORD" name="password"/>
  <property column="PHONE" name="phone"/>
  <property column="EMAIL" name="email"/>
  <property column="ADMIN_STATUS" name="adminStatus"/>
  <property column="DEFAULT_FLOOR" name="defaultFloor"/>
  </class>
</hibernate-mapping>