<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.jmw.conference.entities.Reservation" table="Reservations">
  <id column="RESERVATION_ID" name="id">
   <generator class="native"/>
  </id>
  <property column="USER_ID" name="userId"/>
  <property column="ROOM_ID" name="roomId"/>
  <property column="START_DATE" name="startDate"/>
  <property column="END_DATE" name="endDate"/>
  <property column="COMMENT" name="comment"/>
  </class>
</hibernate-mapping>