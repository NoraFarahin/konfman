<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.jmw.conference.entities.Room" table="ROOMS">
  <id column="ROOM_ID" name="id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="name"/>
  <property column="FLOOR_ID" generated="never" lazy="false" name="floorId"/>
  <property column="BUILDING_ID" generated="never" lazy="false" name="buildingId"/>
  <property generated="never" lazy="false" name="attributes"/>
 </class>
</hibernate-mapping>
