<decorators defaultdir="/decorators">
	<excludes>
        <pattern>/login.jsp*</pattern>
    </excludes>
    <decorator name="default" page="default.jsp">
        <pattern>/*</pattern>
    </decorator>
    <decorator name="appadmin" page="appadmin.jsp">
        <pattern>/appadmin/*</pattern>
    </decorator>
    <decorator name="calendar" page="calendar.jsp">
        <pattern>/cal-*</pattern>
    </decorator>
</decorators>
